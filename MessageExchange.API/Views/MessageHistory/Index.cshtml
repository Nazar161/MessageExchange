@model List<MessageExchange.API.Contracts.MessageResponse>

@{
ViewData["Title"] = "Message History";
}

<h1>Message History</h1>
<table class="table">
    <thead>
    <tr>
        <th>Order Number</th>
        <th>Timestamp</th>
        <th>Content</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var message in Model)
    {
    <tr>
        <td>@message.OrderNumber</td>
        <td>@message.Timestamp</td>
        <td>@message.Content</td>
    </tr>
    }
    </tbody>
</table>
